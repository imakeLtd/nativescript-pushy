<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo" class="page">
  <StackLayout class="p-30">
    <Button text="Get Push Token" tap="{{ doGetDevicePushToken }}" class="button" />
    <Button text="Subscribe to General Topic" tap="{{ subscribeToGeneralTopic }}" class="button" />
    <Button text="Unsubscribe from General Topic" tap="{{ unsubscribeToGeneralTopic }}" class="button" />
    <Button text="Unregister Pushy" tap="{{ doUnregister }}" class="button" />
    <Label text="{{ message }}" class="m-t-15 t-20 text-center c-black" textWrap="true"/>
  </StackLayout>
</Page>